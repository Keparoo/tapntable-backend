{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/companySearch.schema.json",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1
    },
    "categoryId": {
      "type": "integer",
      "minimum": 1
    },
    "modCat": {
      "type": "string",
      "minLength": 1
    },
    "modPrice": {
      "type": "number",
      "minimum": 0
    },
    "isActive": {
      "type": "boolean"
    },
    "desc": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "required": []
}
