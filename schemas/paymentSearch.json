    "isVoid": {
      "type": "boolean"
    },
    "isOpen": {
      "type": "boolean"
    },
    "createdAt": {
      "type": "string",
      "minLength": 1
    },
    "printedAt": {
      "type": "string",
      "minLength": 1
    },
    "start": {
      "type": "string",
      "minLength": 1
    },
    "end": {
      "type": "string",
      "minLength": 1
    },
    "desc": {
      "type": "boolean"
    },
    "closedAt": {
      "type": "string",
      "minLength": 1
