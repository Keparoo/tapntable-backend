{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/companyNew.schema.json",
  "type": "object",
  "properties": {
    "tableNum": {
      "type": "integer",
      "minimum": 1
    },
    "customer": {
      "type": "string",
      "minLength": 1,
      "maxLength": 15
    },
    "numGuests": {
      "type": "integer",
      "minimum": 1
    },
    "printedAt": {
      "type": "string",
      "minimum": 1
    },
    "closedAt": {
      "type": "string",
      "minLength": 1
    },
    "discountId": {
      "type": "integer",
      "minimum": 1
    },
    "subtotal": {
      "type": "number",
      "minimum": 0
    },
    "discountTotal": {
      "type": "number",
      "minimum": 0
    },
    "localTax": {
      "type": "number",
      "minimum": 0
    },
    "stateTax": {
      "type": "number",
      "minimum": 0
    },
    "federalTax": {
      "type": "number",
      "minimum": 0
    },
    "isVoid": {
      "type": "boolean"
    }
  },
  "additionalProperties": false,
  "required": []
}
