{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://our.company.com/companyNew.schema.json",
  "type": "object",
  "properties": {
    "itemId": {
      "type": "integer",
      "minimum": 1
    },
    "orderId": {
      "type": "integer",
      "minimum": 1
    },
    "checkId": {
      "type": "integer",
      "minimum": 1
    },
    "seatNum": {
      "type": "integer",
      "minimum": 1
    },
    "courseNum": {
      "type": "integer",
      "minimum": 1
    },
    "itemNote": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    }
  },
  "additionalProperties": false,
  "required": [ "itemId", "orderId", "checkId" ]
}
